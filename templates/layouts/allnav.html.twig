{% if user %}
    {% if user.isCandidat() %}
        {{ include('layouts/navbar-user.html.twig') }}
    {% elseif user.isMentor() %}
        {{ include('layouts/navbar-mentor.html.twig') }}
    {% elseif user.isAdmin() %}
        {{ include('layouts/navbar-admin.html.twig') }}
